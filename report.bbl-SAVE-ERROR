    \entry{saavedraPerformanceCharacterizationOptimizing1995}{article}{}
      \name{author}{2}{}{%
        {{hash=7f9de5f8f62b7588bace5c43305ab0e1}{%
           family={Saavedra},
           familyi={S\bibinitperiod},
           given={R.H.},
