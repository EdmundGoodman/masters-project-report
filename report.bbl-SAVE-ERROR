      \strng{namehash}{9526d72255ce1ce67d1467dd00d94fa6}
      \strng{fullhash}{9526d72255ce1ce67d1467dd00d94fa6}
      \strng{bibnamehash}{9526d72255ce1ce67d1467dd00d94fa6}
      \strng{authorbibnamehash}{9526d72255ce1ce67d1467dd00d94fa6}
